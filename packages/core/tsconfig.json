{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ES2015",
    "declaration": true,
    "jsx": "react-native",
    "outDir": "./dist",
    "strict": true,
    "baseUrl": "./src",
    "rootDirs": ["./src"],
    "skipLibCheck": true,
